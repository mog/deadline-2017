script.
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject'] = r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-10739913-3', 'auto');
    ga('send', 'pageview');

    if(ga){
        var tracking = [
            {
                category: 'navigation',
                selector: '#navigation a'
            },
            {
                category: 'sponsor',
                selector: '#sponsors a'
            },
            {
                category: 'contact',
                selector: '#contact a'
            }
        ];
        function track(e, t){
            return function() {
                ga('send', {
                    hitType: 'event',
                    eventCategory: tracking[t].category,
                    eventAction: e.href
                });
            }
        };
        for(var t = 0; t < tracking.length - 1; t++) {
            var $elementList = document.querySelectorAll(tracking[t].selector);
            for (var i = 0; i < $elementList.length - 1; i++) {
                $elementList[i].addEventListener('click', track($elementList[i], t), false);
            }
        }
    }